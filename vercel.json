{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/"
        },
        {
            "src": "/resources/(.*)",
            "dest": "/public/resources/"
        },
        {
            "src": "/favicon/(.*)",
            "dest": "/public/favicon/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "/build",
    "env": {
        "APP_NAME": "Laravel",
        "APP_TIMEZONE": "UTC",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://sales-website-chi.vercel.app",
        "APP_KEY": "base64:xnz07KKTjBIHLTcgLyIPmRRDgJ6UuDrp7wk9i8j5jSI=",
        "APP_LOCALE": "en",
        "APP_FALLBACK_LOCALE": "en",
        "APP_FAKER_LOCALE": "en_US",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "file",
        "CACHE_PATH": "/tmp",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VITE_APP_NAME": "${APP_NAME}",
        "NODE_PATH": "node"
    }
}